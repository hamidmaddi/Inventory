    <add name="ServersInventoryDB" connectionString="Server=107NF0310SQL05;Database=ServersInventoryDB;User Id=ServerInventoryUser;Password=TqXTj4sgYj7i;"/>
  </connectionStrings>
  <appSettings>
    <clear/>
    <add key="DestinationProcessed" value="E:\Processed"/>
    <add key="Source" value="E:\Pending"/>
    <add key="DestinationFailed" value="E:\Failed"/>
  </appSettings>
  <system.web>
    <compilation targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
  <system.webServer>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
